package mainMenu;

public abstract class MenuItem {
	public double price;
	public int amount;
	public String itemName;
	public String itemType;
	public  double itemPrice() {
		return price;
	}
	public int getAmount() {
		return amount;
	}
	public String getItemName() {
		return itemName;
	}
	public String getType() {
		return itemType;
	}
}
